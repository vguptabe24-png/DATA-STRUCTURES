#include <iostream>
using namespace std;
int main(){
int n , element;
cin>>n;
int arr[50];
for(int i = 0; i<n; i++) cin>>arr[i];
cin>>element;

int start= 0 , end = n-1;
while(start<=end){
int mid = (start + end)/2;
if(arr[mid] == element) {cout<<mid; return 0; }
if(arr[mid] < element) start = mid + 1;
else end = mid-1;
}
cout<<"NOT FOUND"<<endl;
}
