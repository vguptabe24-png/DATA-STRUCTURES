#include <stdio.h>
#define SIZE 5

int queue[SIZE];
int front = -1, rear = -1;

void enqueue(int x) {
    if ((front == 0 && rear == SIZE-1) || (rear+1) % SIZE == front) {
        printf("Queue FULL\n");
    } else {
        if (front == -1) front = 0;
        rear = (rear+1) % SIZE;
        queue[rear] = x;
        printf("%d added\n", x);
    }
}

void dequeue() {
    if (front == -1) {
        printf("Queue EMPTY\n");
    } else {
        printf("%d removed\n", queue[front]);
        if (front == rear) front = rear = -1;
        else front = (front+1) % SIZE;
    }
}

void peek() {
    if (front == -1) printf("Queue EMPTY\n");
    else printf("Front = %d\n", queue[front]);
}

void display() {
    if (front == -1) {
        printf("Queue EMPTY\n");
    } else {
        int i = front;
        printf("Queue: ");
        while (1) {
            printf("%d ", queue[i]);
            if (i == rear) break;
            i = (i+1) % SIZE;
        }
        printf("\n");
    }
}

int main() {
    int ch, val;
    while (1) {
        printf("\n1.Enqueue 2.Dequeue 3.Peek 4.Display 5.Exit\n");
        scanf("%d", &ch);
        if (ch == 1) { printf("Enter: "); scanf("%d",&val); enqueue(val); }
        else if (ch == 2) dequeue();
        else if (ch == 3) peek();
        else if (ch == 4) display();
        else if (ch == 5) break;
        else printf("Invalid\n");
    }
    return 0;
}
