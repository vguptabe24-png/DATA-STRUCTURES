#include <iostream>
using namespace std;

int main() {
    char str[100];
    cin >> str;

    int freq[26] = {0};

    // count frequency of each character
    for (int i = 0; str[i] != '\0'; i++) {
        freq[str[i] - 'a']++;
    }

    // check if any letter appears 3 or more times
    for (int i = 0; i < 26; i++) {
        if (freq[i] >= 3) {
            cout << "YES\n";
            return 0;
        }
    }

    cout << "NO\n";
    return 0;
}
