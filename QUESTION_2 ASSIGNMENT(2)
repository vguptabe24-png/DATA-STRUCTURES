#include <iostream>
using namespace std;

int main() {
    int arr[] = {64, 34, 25, 12, 22, 11, 90};
    int n = 7; // number of elements

    for (int i = 0; i < n - 1; i++) { // Number of passes!!
        for (int j = 0; j < n - i - 1; j++) { // Comparing adjacent elements!!
            if (arr[j] > arr[j + 1]) {
                // Swap elements
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
    for (int i = 0; i < n; i++) { // printing sorted array!!
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}
