#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter number of strings: ";
    cin >> n;

    char str[10][50];   // 10 strings, max length 50

    cout << "Enter " << n << " strings:\n";
    for (int i = 0; i < n; i++) {
        cin.getline(str[i], 50);   // reads whole line (can include spaces)
    }

    for (int i = 0; i < n - 1; i++) {     // Bubble sort
        for (int j = i + 1; j < n; j++) {
            if (strcmp(str[i], str[j]) > 0) { // compare strings
                char temp[50];
                strcpy(temp, str[i]);
                strcpy(str[i], str[j]);
                strcpy(str[j], temp);
            }
        }
    }

    cout << "\nStrings in alphabetical order:\n";
    for (int i = 0; i < n; i++) {
        cout << str[i] << endl;
    }

    return 0;
}
