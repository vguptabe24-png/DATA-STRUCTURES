#include <iostream>
using namespace std;

int main() {
    char str1[201], str2[201];
    cout << "Enter first string: ";
    cin.getline(str1, 200);
    cout << "Enter second string: ";
    cin.getline(str2, 200);

    // Count arrays for all ASCII characters
    int freq1[256] = {0};
    int freq2[256] = {0};

    // Fill counts for str1
    for (int i = 0; str1[i] != '\0'; i++) {
        unsigned char ch = str1[i];
        freq1[ch]++;
    }

    // Fill counts for str2
    for (int i = 0; str2[i] != '\0'; i++) {
        unsigned char ch = str2[i];
        freq2[ch]++;
    }

    // Compare counts
    bool isAnagram = true;
    for (int i = 0; i < 256; i++) {
        if (freq1[i] != freq2[i]) {
            isAnagram = false;
            break;
        }
    }

    if (isAnagram) cout << "Anagram\n";
    else           cout << "Not an anagram\n";

    return 0;
}
